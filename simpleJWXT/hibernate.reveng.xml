	</table>
	<table name="score" catalog="db_simplejwxt" class="entity.Score">
		<primary-key>
			<generator class="assigned"></generator>
		</primary-key>
	</table>
	<table name="student" catalog="db_simplejwxt"
		class="entity.Student">
		<primary-key>
			<generator class="assigned"></generator>
		</primary-key>
	</table>
<table name="teacher" catalog="db_simplejwxt" class="entity.Teacher">
	<primary-key>
		<generator class="assigned"></generator>
	</primary-key>
</table>
